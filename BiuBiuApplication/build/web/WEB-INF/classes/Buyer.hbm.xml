<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.biubiu.model.Buyer" table="Buyer">
      <id name="buy_id" type="string">
          <column name="buy_id"/>
      </id>
      <property name="buy_name" type="string">
          <column name="buy_name"/>
      </property>
      <property name="buy_pass" type="string">
          <column name="buy_pass"/>
      </property>
      <property name="buy_level" type="string">
          <column name="buy_level"/>
      </property>
      <property name="buy_times" type="integer">
          <column name="buy_times"/>
      </property>
  </class>
</hibernate-mapping>

