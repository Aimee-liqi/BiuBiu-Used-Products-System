<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.biubiu.model.Comment"  table="Comment" >
        <id name="com_id" column="com_id"  type="integer">
            <generator class="identity"/>
        </id>
        <property name="com_detail" type="string">
            <column name="com_detail"/>
        </property>
        <property name="com_type" type="string">
            <column name="com_type"/>
        </property>
        <property name="pro_id" type="integer">
            <column name="pro_id"/>
        </property>
    </class>
  

</hibernate-mapping>
